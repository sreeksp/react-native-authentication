{
    "parser": "babel-eslint",
    "env": {
        "browser": true
    },
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "extends": [
        "rallycoding",
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "rules": {
        "no-console": 0,
        "react/prop-types": 0,
        "react/require-extension": 0
    }
}